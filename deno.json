{
  "imports": {
    "lume/": "https://deno.land/x/lume@v2.4.3/",
    "blog/": "https://deno.land/x/lume_theme_simple_blog@v0.15.10/",
    "lume/cms/": "https://cdn.jsdelivr.net/gh/lumeland/cms@0.8.1/"
  },
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run -A -",
    "build": "deno task lume",
    "serve": "deno task lume -s",
    "cms": "deno task lume cms",
    "lup": "deno task lume upgrade",
    "up": "deno run -A --quiet 'https://deno.land/x/nudd@v0.2.8/cli.ts' update deno.json"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ]
  }
}
